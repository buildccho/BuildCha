{
  "name": "backend",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": "./dist/client.js",
    "./types": "./dist/client.d.ts"
  },
  "scripts": {
    "dev": "wrangler dev --port 8787 --ip 0.0.0.0",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "local:migration": "wrangler d1 migrations apply buildcha --local",
    "remote:migration": "wrangler d1 migrations apply buildcha --remote",
    "test": "jest",
    "build": "pnpm build:prisma && pnpm build:tsc",
    "build:tsc": "tsc",
    "build:prisma": "npx prisma generate",
    "create-vectorize": "npx wrangler vectorize create buildcha-vector  --dimensions 1536 --metric cosine",
    "delete-vectorize": "npx wrangler vectorize delete buildcha-vector"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.2",
    "@langchain/cloudflare": "^0.1.1",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.77",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.9",
    "@langchain/textsplitters": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@prisma/adapter-d1": "^6.15.0",
    "@prisma/client": "^6.15.0",
    "@scalar/hono-api-reference": "^0.9.18",
    "better-auth": "^1.3.8",
    "hono": "^4.9.6",
    "hono-openapi": "0.4.8",
    "pdf-parse": "^2.1.7",
    "pdfjs-serverless": "^1.1.0",
    "zod": "^4.0.17",
    "zod-openapi": "^5.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250913.0",
    "@jest/globals": "^30.1.2",
    "@types/jest": "^29.4.3",
    "jest": "^29.4.3",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.3",
    "typescript": "^5",
    "wrangler": "^4.4.0"
  }
}
